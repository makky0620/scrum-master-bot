# Discord Bot Configuration
# Replace with your actual bot token from Discord Developer Portal
DISCORD_TOKEN=your_discord_bot_token
# Client ID of your bot application
CLIENT_ID=your_client_id
# Guild ID where you want to register commands (for development)
GUILD_ID=your_guild_id

# Backlog Configuration
# Your Backlog space host (e.g., yourspace.backlog.jp)
BACKLOG_HOST=your_backlog_host
# Your Backlog API key
BACKLOG_API_KEY=your_backlog_api_key
# Your Backlog project key (e.g., PROJECT)
BACKLOG_PROJECT_KEY=your_backlog_project_key

# Node environment
NODE_ENV=production
